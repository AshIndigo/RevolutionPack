<ConfigSection>

    <!-- Remove all ores we're going to replace and can't stop the mod from placing -->
    <ConfigSection>
        <Substitute name='RemoveEmeralds' block='minecraft:stone'>
            <Replaces block='minecraft:emerald_ore' weight='1.0' />
            <Replaces block='ReactorCraft:reactorcraft_block_fluoriteore' weight='1.0' />
            <Replaces block='ReactorCraft:reactorcraft_block_fluoriteore:1' weight='1.0' />
            <Replaces block='ReactorCraft:reactorcraft_block_fluoriteore:2' weight='1.0' />
            <Replaces block='ReactorCraft:reactorcraft_block_fluoriteore:3' weight='1.0' />
            <Replaces block='ReactorCraft:reactorcraft_block_fluoriteore:4' weight='1.0' />
            <Replaces block='ReactorCraft:reactorcraft_block_fluoriteore:5' weight='1.0' />
            <Replaces block='ReactorCraft:reactorcraft_block_fluoriteore:6' weight='1.0' />
            <Replaces block='ReactorCraft:reactorcraft_block_fluoriteore:7' weight='1.0' />
            <Replaces block='ReactorCraft:reactorcraft_block_ore:1' weight='1.0' />
            <Replaces block='ReactorCraft:reactorcraft_block_ore:2' weight='1.0' />
            <Replaces block='ReactorCraft:reactorcraft_block_ore:3' weight='1.0' />
            <Replaces block='ReactorCraft:reactorcraft_block_ore:4' weight='1.0' />
            <Replaces block='ReactorCraft:reactorcraft_block_ore:7' weight='1.0' />
            <Replaces block='ReactorCraft:reactorcraft_block_ore:8' weight='1.0' />
        </Substitute>
    </ConfigSection>
            
    <ConfigSection>
        <Veins name='ApatiteVeins' inherits='PresetSparseVeins'>
            <Description>
                Apatite spawns in large, rare veins between y-level 56 and 184
            </Description>
            <OreBlock block='Forestry:resources' weight='1.0' />
            <Replaces block='minecraft:stone' weight='1.0' />
            <Biome name='.*'  />
            <Setting name='MotherlodeFrequency' avg='0.0125'/>
            <Setting name='MotherlodeSize' avg='0' range='0' type='normal' />
            <Setting name='MotherlodeHeight' avg='120' range='64' type='uniform'/>
            <Setting name='BranchLength' avg='200' range='100' type='normal' />
            <Setting name='SegmentRadius' avg='2.5' range='1.27' type='normal' />
        </Veins>
    </ConfigSection>
        
    <ConfigSection>
        <Veins name='CadmiumVeins' inherits='PresetLayeredVeins'>
            <Description>
                Cadmium spawns in layered veins between y-level 2 and 14
            </Description>
            <OreBlock block='ReactorCraft:reactorcraft_block_ore:2' weight='1.0' />
            <Replaces block='minecraft:stone' weight='1.0' />
            <Biome name='.*'  />
            <Setting name='MotherlodeFrequency' avg='0.025' range='0' type='normal' />
            <Setting name='MotherlodeSize' avg='1.0' range='0.5' type='normal' />
            <Setting name='MotherlodeHeight' avg='8' range='6' type='normal'/>
            <Setting name='BranchLength' avg='79' range='39.5' type='normal' />
            <Setting name='SegmentRadius' avg='0.5' range='0.3' type='normal' />
        </Veins>
    </ConfigSection>
    
    <ConfigSection>
        <Veins name='IndiumVeins'  inherits='PresetLayeredVeins'>
            <Description>
                Indium spawns in layered veins between y-level 6 and 18
            </Description>
            <OreBlock block='ReactorCraft:reactorcraft_block_ore:3' weight='1.0' />
            <Replaces block='minecraft:stone' weight='1.0' />
            <Biome name='.*'  />
            <Setting name='MotherlodeFrequency' avg='0.025' range='0' type='normal'/>
            <Setting name='MotherlodeSize' avg='2.5' range='1' type='normal' />
            <Setting name='MotherlodeHeight' avg='12' range='6' type='normal'/>
            <Setting name='BranchLength' avg='60' range='30' type='normal' />
            <Setting name='SegmentRadius' avg='0.5' range='0.3' type='normal' />
        </Veins>
    </ConfigSection>    
    
    <ConfigSection>
        <Veins name='FluoriteVeins' inherits='PresetHugeVeins'>
            <Description>
                Fluorite spawns in large motherloads between y-level 3 and 13 with large tentacles spreading out for long distances
            </Description>
            <OreBlock block='ReactorCraft:reactorcraft_block_fluoriteore:0' weight='0.125' />
            <OreBlock block='ReactorCraft:reactorcraft_block_fluoriteore:1' weight='0.125' />
            <OreBlock block='ReactorCraft:reactorcraft_block_fluoriteore:2' weight='0.125' />
            <OreBlock block='ReactorCraft:reactorcraft_block_fluoriteore:3' weight='0.125' />
            <OreBlock block='ReactorCraft:reactorcraft_block_fluoriteore:4' weight='0.125' />
            <OreBlock block='ReactorCraft:reactorcraft_block_fluoriteore:5' weight='0.125' />
            <OreBlock block='ReactorCraft:reactorcraft_block_fluoriteore:6' weight='0.125' />
            <OreBlock block='ReactorCraft:reactorcraft_block_fluoriteore:7' weight='0.125' />
            <Replaces block='minecraft:stone' weight='1.0' />
            <Biome name='.*'/>
            <Setting name='MotherlodeFrequency' avg='0.00625' range='0.002' type='normal' />
            <Setting name='MotherlodeSize' avg='2.5' range='1.25' type='normal'/>
            <Setting name='MotherlodeHeight' avg='8' range='5' type='normal' />
            <Setting name='BranchHeightLimit' avg='24'/>
        </Veins>
    </ConfigSection>

    <ConfigSection>
        <Veins name='MagnetiteVeins' inherits='PresetLayeredVeins'>
            <Description>
                Magnetite spawns in large, rare veins between y-level 76 and 98
            </Description>
            <OreBlock block='ReactorCraft:reactorcraft_block_ore:8' weight='1.0' />
            <Replaces block='minecraft:stone' weight='1.0' />
            <Biome name='.*'  />
            <Setting name='MotherlodeHeight' avg='87' range='11' type='normal' />
            <Setting name='MotherlodeFrequency' avg='0.075' range='0.05' type='normal' />
            <Setting name='BranchHeightLimit' avg='8' range='8' type='normal' />
        </Veins>
    </ConfigSection>
    
    <ConfigSection>
        <Veins name='CalciteVeins' inherits='PresetLayeredVeins'>
            <Description>
                Calcite spawns in rare veins between y-level 42 and 58
            </Description>
            <OreBlock block='ReactorCraft:reactorcraft_block_ore:7' weight='1.0' />
            <Replaces block='minecraft:stone' weight='1.0' />
            <Biome name='.*'  />
            <Setting name='MotherlodeFrequency' avg='0.0125'/>
            <Setting name='MotherlodeSize' avg='0' range='0' type='normal' />
            <Setting name='MotherlodeHeight' avg='50' range='8' type='normal' />
            <Setting name='BranchLength' avg='50' range='25' type='normal' />
            <Setting name='SegmentRadius' avg='1.5' range='0.75' type='normal' />
        </Veins>
    </ConfigSection>  
    
    <ConfigSection>
        <Veins name='CertusVeins' inherits='PresetVerticalVeins'>
            <Description>
                Certus quartz spawns with charged certus quarts in vertical veins between y-level 76 and 52
            </Description>
            <OreBlock block='appliedenergistics2:tile.OreQuartz' weight='0.85' />
            <OreBlock block='appliedenergistics2:tile.OreQuartzCharged' weight='0.15' />
            <Replaces block='minecraft:stone' weight='1.0' />
            <Biome name='.*'/>
            <Setting name='MotherlodeFrequency' avg='0.08' range='0.02' type='normal'/>
            <Setting name='MotherlodeHeight' avg='64' range='12' type='normal'/>
            <Setting name='BranchLength' avg='32' range='6' type='normal' />           
        </Veins>
    </ConfigSection>
        
</ConfigSection>

